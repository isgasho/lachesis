[
    {
        "name": "wordpress",
        "protocol": "http/s",
        "options": {
            "ports": [
                80,
                443,
                8080,
                8081,
                8090
            ]
        },
        "service": {
            "regex": "(?m)<meta name=\"generator\" content=\"WordPress ",
            "log": false
        },
        "versions": {
            "semver": {
                "regex": "(?m)<meta name=\"generator\" content=\"WordPress ",
                "ranges": [
                    {
                        "from": "4.3.0",
                        "to": "4.3.6",
                        "description": "VULNERABLE! [CVE-2016-10033]"
                    },
                    {
                        "from": "4.4.0",
                        "to": "4.4.5",
                        "description": "VULNERABLE! [CVE-2016-10033]"
                    },
                    {
                        "from": "4.5.0",
                        "to": "4.5.4",
                        "description": "VULNERABLE! [CVE-2016-10033]"
                    },
                    {
                        "from": "4.6.0",
                        "to": "4.6.1",
                        "description": "VULNERABLE! [CVE-2016-10033]"
                    },
                    {
                        "from": "4.7.0",
                        "to": "4.7.0",
                        "description": "VULNERABLE! [CVE-2016-10033]"
                    }
                ]
            }
        }
    },
    {
        "name": "vnc",
        "protocol": "tcp/custom",
        "options": {
            "ports": [5900],
            "timeout": true,
            "message": "RFB 003.008\n"
        },
        "service": {
            "regex": "RFB ",
            "log": true
        },
        "versions": {
            "regex": [
                {
                    "regex": "RFB 004.001",
                    "version": "004.001",
                    "description": "Version 004.001"
                },
                {
                    "regex": "RFB 003.008",
                    "version": "003.008",
                    "description": "Version 003.008"
                }
            ]
        }
    },
    {
        "name": "sshd",
        "protocol": "tcp/custom",
        "options": {
            "ports": [22],
            "timeout": false,
            "message": "Could you kindly tell me if you are sshd?\r\n"
        },
        "service": {
            "regex": "SSH-",
            "log": false
        },
        "versions": {
            "regex": [
                {
                    "regex": "OpenSSH_5.",
                    "version": "OpenSSH 5.x",
                    "description": "OpenSSH 5.x (outdated)"
                },
                {
                    "regex": "OpenSSH_6.",
                    "version": "OpenSSH 6.x",
                    "description": "OpenSSH 6.x (outdated)"
                }
            ]
        }
    },
    {
        "name": "redis",
        "protocol": "tcp/custom",
        "options": {
            "ports": [6379],
            "timeout": true,
            "message": "PING\r\n"
        },
        "service": {
            "regex": "\\+PONG|\\-NOAUTH",
            "log": true
        },
        "versions": {
            "regex": [
                {
                    "regex": "PONG",
                    "version": "Unprotected instance (no password)",
                    "description": "Unprotected instance (no password)"
                }
            ]
        }
    },
    {
        "name": "webcams",
        "protocol": "http/s",
        "options": {
            "ports": [
                80,
                81,
                82,
                83,
                88,
                443,
                5001,
                8010,
                8060,
                8080,
                8081,
                8083,
                9000,
                3000
            ]
        },
        "service": {
            "regex": "IPCamera_Logo|MJPG-Streamer|(?i)webcam server(?-i)",
            "log": false
        },
        "versions": {
            "regex": [
                {
                    "regex": "IPCamera_Logo",
                    "version": "MayGion IP camera (admin:admin)",
                    "description": "MayGion IP camera (admin:admin)"
                },
                {
                    "regex": "MJPG-Streamer",
                    "version": "MJPG-Streamer",
                    "description": "MJPG-Streamer"
                },
                {
                    "regex": "(?i)webcam server(?-i)",
                    "version": "Android webcam",
                    "description": "Android webcam"
                }
            ]
        }
    }
]